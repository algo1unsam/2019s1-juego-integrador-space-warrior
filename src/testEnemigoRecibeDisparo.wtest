import enemigos.*
import wollok.game.*
import nave.*

describe "Tests Enemigo Nivel 1 / Clase Enemigo" {

	const enemigo = new Enemigo(position = game.at(5, 5), posiciones = 1)

	test "Recibir disparo con vida > 0 " {
		// Valor default de vida = 20
		enemigo.recibeDisparo()
		assert.that(enemigo.vida() == 15)
	}

}

